{
    "configurations": [
        {
            "name": "Linux",
            "defines": [
                "_DEBUG",
                "MATLABPOOL_DISP_WORKER_OUTPUT",
                "MATLABPOOL_DISP_WORKER_ERROR"
            ],
            "compilerPath": "/usr/bin/g++", 
            "intelliSenseMode": "gcc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "/usr/local/MATLAB/R2019b/extern/include/"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "includePath": [
                "${default}",
                "/usr/local/MATLAB/R2019b/extern/include",
                "./src",
                "./include"
            ],
            "cStandard": "c11",
            "cppStandard": "gnu++17"
        }
    ],
    "version": 4
}